    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "babel-node": "^0.0.1-security",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5"
  },
  "bugs": {
    "url": "https://github.com/alejandrogalaz21/express-api/issues"
  },
  "homepage": "https://github.com/alejandrogalaz21/express-api#readme",
