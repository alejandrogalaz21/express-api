{
  "name": "express-api",
  "version": "1.0.0",
  "description": "simple express api demo",
  "main": "index.js",
  "scripts": {
    "start": "env ENV=development nodemon index.js --exec babel-node",
    "build": "env ENV=production babel src -d dist",
